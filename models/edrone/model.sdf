      <prop_kp> 1 </prop_kp>
      <prop_ki> 0 </prop_ki>
      <prop_kd> 0.0 </prop_kd>
      <robotNamespace>edrone</robotNamespace>
      <bodyName_1>prop1</bodyName_1>
      <bodyName_2>prop2</bodyName_2>
      <bodyName_3>prop3</bodyName_3>
      <bodyName_4>prop4</bodyName_4>
